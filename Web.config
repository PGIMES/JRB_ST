	<connectionStrings>
		<add name="ApplicationServices" connectionString="data source=.;database=JRB_ST;user id=sa;password=sasa" providerName="System.Data.SqlClient"/>
	</connectionStrings>
	<system.web>
		<customErrors mode="Off">
